library(rvest)
url<-'https://nn.esf.fang.com/'
read_html(url,encoding = 'GBK')->webpage
html_nodes(webpage,'.tit_shop')%>%html_text()->title
html_nodes(webpage,'.red+ span')%>%html_text()->cost
html_nodes(webpage,'.add_shop span')%>%html_text()->location
data.frame(title,cost,location)->fangjia
view(fangjia)
